    "@babel/plugin-transform-runtime": "^7.8.3",
    "babel-loader": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
